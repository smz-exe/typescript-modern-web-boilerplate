{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["DOM", "DOM.Iterable", "ES2020"],
      "module": "ESNext",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "jsx": "react-jsx",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "types": ["vite/client"]
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}